<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppinglistmanager">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <meta-data android:name="DATABASE" android:value="article.db" />
    <meta-data android:name="VERSION" android:value="9" />
    <meta-data android:name="QUERY_LOG" android:value="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LastPage"></activity>
        <activity android:name=".ShoppingListEditor" />
        <activity
            android:name=".ShowListArticle"
            android:label="Article List"
            android:parentActivityName=".ShowListArticle" />
        <activity android:name=".ListEditor" />
        <activity android:name=".ShoppingListShow" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".Slide" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>